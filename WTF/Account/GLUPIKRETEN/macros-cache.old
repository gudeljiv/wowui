/run for b=0,4 do for s=1,GetContainerNumSlots(b) do l=GetContainerItemLink(b,s) if l then _,_,r=GetItemInfo(l) if (r==0) then PickupContainerItem(b,s) DeleteCursorItem() end end end end
END
VER 3 0000000000000009 "DELETE ITEM" "INV_MISC_QUESTIONMARK"
/run for b=0,4 do for s=1,36 do n=GetContainerItemLink(b,s);if n and string.find(n,"Sharp Arrow") then PickupContainerItem(b,s);DeleteCursorItem();end;end;end;
END

<Bindings>
  <!-- Toggle options -->
  <Binding name="DEJUNK_TOGGLE_OPTIONS" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_ToggleOptions()
  </Binding>

  <!-- Start destroying -->
  <Binding name="DEJUNK_START_DESTROYING" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_StartDestroying()
  </Binding>

  <!-- Add/rem inclusions -->
  <Binding name="DEJUNK_ADD_INCLUSIONS" header="DEJUNKBLANK1" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_AddToInclusions()
  </Binding>
  <Binding name="DEJUNK_REM_INCLUSIONS" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_RemoveFromInclusions()
  </Binding>

  <!-- Add/rem exclusions -->
  <Binding name="DEJUNK_ADD_EXCLUSIONS" header="DEJUNKBLANK2" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_AddToExclusions()
  </Binding>
  <Binding name="DEJUNK_REM_EXCLUSIONS" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_RemoveFromExclusions()
  </Binding>

  <!-- Add/rem destroyables -->
  <Binding name="DEJUNK_ADD_DESTROYABLES" header="DEJUNKBLANK3" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_AddToDestroyables()
  </Binding>
  <Binding name="DEJUNK_REM_DESTROYABLES" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_RemoveFromDestroyables()
  </Binding>
</Bindings>

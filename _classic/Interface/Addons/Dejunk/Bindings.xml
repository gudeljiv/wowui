<Bindings>
  <!-- General -->
  <Binding name="DEJUNK_TOGGLE_OPTIONS_FRAME" header="DEJUNK_HEADER_GENERAL" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_ToggleOptionsFrame()
  </Binding>
  <Binding name="DEJUNK_TOGGLE_SELL_FRAME" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_ToggleSellFrame()
  </Binding>
  <Binding name="DEJUNK_TOGGLE_DESTROY_FRAME" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_ToggleDestroyFrame()
  </Binding>
  <Binding name="DEJUNK_OPEN_LOOTABLES" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_OpenLootables()
  </Binding>

  <!-- Sell -->
  <Binding name="DEJUNK_START_SELLING" header="DEJUNK_HEADER_SELL" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_StartSelling()
  </Binding>
  <Binding name="DEJUNK_SELL_NEXT_ITEM" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_SellNextItem()
  </Binding>

  <!-- Destroy -->
  <Binding name="DEJUNK_START_DESTROYING" header="DEJUNK_HEADER_DESTROY" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_StartDestroying()
  </Binding>
  <Binding name="DEJUNK_DESTROY_NEXT_ITEM" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_DestroyNextItem()
  </Binding>

  <!-- Lists -->
  <Binding name="DEJUNK_ADD_INCLUSIONS" header="DEJUNK_HEADER_LISTS" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_AddToList("sell", "inclusions", "profile")
  </Binding>
  <Binding name="DEJUNK_ADD_INCLUSIONS_GLOBAL" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_AddToList("sell", "inclusions", "global")
  </Binding>
  <Binding name="DEJUNK_REM_INCLUSIONS" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_RemoveFromList("sell", "inclusions", "profile")
  </Binding>
  <Binding name="DEJUNK_REM_INCLUSIONS_GLOBAL" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_RemoveFromList("sell", "inclusions", "global")
  </Binding>

  <Binding name="DEJUNK_ADD_EXCLUSIONS" header="DEJUNK_HEADER_BLANK1" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_AddToList("sell", "exclusions", "profile")
  </Binding>
  <Binding name="DEJUNK_ADD_EXCLUSIONS_GLOBAL" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_AddToList("sell", "exclusions", "global")
  </Binding>
  <Binding name="DEJUNK_REM_EXCLUSIONS" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_RemoveFromList("sell", "exclusions", "profile")
  </Binding>
  <Binding name="DEJUNK_REM_EXCLUSIONS_GLOBAL" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_RemoveFromList("sell", "exclusions", "global")
  </Binding>

  <Binding name="DEJUNK_ADD_DESTROYABLES" header="DEJUNK_HEADER_BLANK2" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_AddToList("destroy", "inclusions", "profile")
  </Binding>
  <Binding name="DEJUNK_ADD_DESTROYABLES_GLOBAL" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_AddToList("destroy", "inclusions", "global")
  </Binding>
  <Binding name="DEJUNK_REM_DESTROYABLES" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_RemoveFromList("destroy", "inclusions", "profile")
  </Binding>
  <Binding name="DEJUNK_REM_DESTROYABLES_GLOBAL" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_RemoveFromList("destroy", "inclusions", "global")
  </Binding>

  <Binding name="DEJUNK_ADD_UNDESTROYABLES" header="DEJUNK_HEADER_BLANK3" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_AddToList("destroy", "exclusions", "profile")
  </Binding>
  <Binding name="DEJUNK_ADD_UNDESTROYABLES_GLOBAL" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_AddToList("destroy", "exclusions", "global")
  </Binding>
  <Binding name="DEJUNK_REM_UNDESTROYABLES" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_RemoveFromList("destroy", "exclusions", "profile")
  </Binding>
  <Binding name="DEJUNK_REM_UNDESTROYABLES_GLOBAL" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_RemoveFromList("destroy", "exclusions", "global")
  </Binding>
</Bindings>

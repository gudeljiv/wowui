["init"] = {
["custom"] = "aura_env.runOnce = aura_env.runOnce or 0\n\nif aura_env.runOnce == 0 then\n    aura_env.runOnce = aura_env.runOnce + 1\n    \n    -- remove event triggers that were auto-closing guild tab\n    local events = {\"GUILD_ROSTER_UPDATE\", \"GUILD_TRADESKILL_UPDATE\", \"GUILD_RANKS_UPDATE\"}\n    for i=1,#events do\n        FriendsFrame:UnregisterEvent(events[i])\n    end\n    -- reactivate the guild tab button\n    PanelTemplates_SetNumTabs(FriendsFrame,4)\n    PanelTemplates_EnableTab(FriendsFrame,4)\n    PanelTemplates_ShowTab(FriendsFrame,4)\n    \n    -- overload OnClick of tab buttons to allow for guild tab\n    FriendsFrameTab1:HookScript(\"OnClick\", function()\n            PanelTemplates_ShowTab(FriendsFrame,4)\n    end)\n    FriendsFrameTab2:HookScript(\"OnClick\", function()\n            PanelTemplates_ShowTab(FriendsFrame,4)\n    end)\n    FriendsFrameTab3:HookScript(\"OnClick\", function()\n            PanelTemplates_ShowTab(FriendsFrame,4)\n    end)\n    FriendsFrameTab4:HookScript(\"OnClick\", function()\n            PanelTemplates_ShowTab(FriendsFrame,4)\n            PanelTemplates_SetTab(FriendsFrame,4)\n            PanelTemplates_UpdateTabs(FriendsFrame)\n    end)\n    \n    -- need to reactivate guild tab every time we open the social frame   \n    FriendsFrame:HookScript(\"OnShow\", function()\n            PanelTemplates_SetNumTabs(FriendsFrame,4)\n            PanelTemplates_EnableTab(FriendsFrame,4)\n            PanelTemplates_ShowTab(FriendsFrame,4)\n    end)\n    \n    -- need a better way to do this\n    -- all scripts on the guild tab seem to try to close it?\n    -- current workaround is to keep it open every time the UI is drawn    \n    GuildFrame:HookScript(\"OnUpdate\", function()\n            if GuildFrame:IsShown() then\n                PanelTemplates_SetTab(FriendsFrame,4)\n            end\n    end)\n    \n    -- Close Communities frame when we open Guild frame\n    GuildFrame:HookScript(\"OnShow\", function()\n            if CommunitiesFrame and CommunitiesFrame:IsShown() then\n                HideUIPanel(CommunitiesFrame)\n            end\n    end)\nend\n\n-- Close Guild frame when we open Communities frame\naura_env.EventFrame = CreateFrame(\"Frame\", nil)\naura_env.EventFrame:RegisterEvent(\"ADDON_LOADED\")\naura_env.EventFrame:SetScript(\"OnEvent\", function(self, event, ...)\n        if event == \"ADDON_LOADED\" then\n            local args = ...\n            if args and args == \"Blizzard_Communities\" then\n                CommunitiesFrame:HookScript(\"OnShow\", function() \n                        if GuildFrame and GuildFrame:IsShown() then\n                            HideUIPanel(FriendsFrame)\n                        end\n                end)\n            end\n        end\nend)",
